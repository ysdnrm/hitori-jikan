<%= stylesheet_link_tag "posts", :media => "all" %>
<% if obj.errors.any? %>
  <div id="error_explanation">
    <h3><%= t("errors.template.header", model: Post.model_name.human, count: obj.errors.count) %></h3>
    <ul>
      <% obj.errors.full_messages.each do |message| %>
        <li><%= t message %></li>
      <% end %>
    </ul>
  </div>
<% end %>